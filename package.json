{
  "name": "react-ui-comments",
  "version": "0.0.0",
  "description": "Vercel Preview Comments の OSS 版。UI上で直接コメントを残し、AIエージェントに構造化して渡せる。",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.js"
    }
  },
  "bin": {
    "react-ui-comments": "./dist/cli.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsup",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "comments",
    "preview",
    "feedback",
    "ui",
    "design",
    "collaboration",
    "ai",
    "agent",
    "mcp"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
