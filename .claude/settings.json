{
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "$CLAUDE_PROJECT_DIR/.claude/hooks/session-start.sh"
          }
        ]
      }
    ]
  },
  "extraKnownMarketplaces": {
    "opentoolbar-plugins": {
      "source": {
        "source": "directory",
        "path": "./.claude/plugins"
      }
    }
  },
  "enabledPlugins": {
    "compound@opentoolbar-plugins": true
  }
}
